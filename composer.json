{
    "name": "neimheadh/solid-bundle",
    "description": "SOLID development helper for symfony projects.",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Neimheadh\\SolidBundle\\": "src/",
            "Neimheadh\\SolidBundle\\Tests\\": "tests/src/"
        }
    },
    "authors": [
        {
            "name": "Mathieu Wambre",
            "email": "contact@neimheadh.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/http-kernel": "^6.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.15",
        "doctrine/doctrine-bundle": "^2.10",
        "symfony/test-pack": "^1.1",
        "polishsymfonycommunity/symfony-mocker-container": "^1.0",
        "symfony/event-dispatcher": "^6.3",
        "friendsofphp/php-cs-fixer": "^3.17",
        "sonata-project/user-bundle": "^5.9",
        "symfony/twig-bundle": "^6.3",
        "symfony/security-bundle": "^6.3"
    }
}
